{
    "builds": [
      {
        "src": "app/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app/app.py"
      }
    ]
    ,
    "git": {
      "deploymentEnabled": {
        "master": true,
        "main": false
      }
    }
}