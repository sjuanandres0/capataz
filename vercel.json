{
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "functions": {
        "api/index.py": {
            "runtime": "python3.8",
            "includeFiles": "api/**"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ]
}